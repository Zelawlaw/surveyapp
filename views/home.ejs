<head>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.3/jquery.min.js"></script>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
  <link href="/css/styles.css" rel="stylesheet" type="text/css" /></head>
<body class="home">
  <h3> <%= msg %> </h3>
  <table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">First Name </th>
      <th scope="col">Surname</th>
      <th scope="col">Date of Survey</th>
      <th scope="col">Name of Survey</th>
    </tr>
  </thead>
  <tbody>
    <%
    for(let i=0;i<samplesurveys.length;i++){
    %>
    
      <tr class='clickable-row' data-href=<%= '/details/'+samplesurveys[i]['_id'] %> >
    
        <th scope="row"><%= i+1 %></th>
      <td><%= samplesurveys[i]['firstName'] %></td>
      <td><%= samplesurveys[i]['surname'] %></td>
      <td><%= samplesurveys[i]['surveyDate'] %></td>
      <td><%= samplesurveys[i]['surveyname'] %></td>
      <!-- <td><%= samplesurveys[i]['']%></td> -->

    </tr>
  
    <% } %>
  </tbody>
</table>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
<script>
  jQuery(document).ready(function($) {
    $(".clickable-row").click(function() {
      console.log($(this).data("href"))
        window.location = $(this).data("href");
    });
});
</script>
</body>
<head>
  <link
    href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css"
    rel="stylesheet"
    integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD"
    crossorigin="anonymous"
  />
  <link href="/css/styles.css" rel="stylesheet" type="text/css" />
</head>
<body class="newSurvey">
  <h2>Home OwnerShip Survey</h2>
  <p id="msg"><%= msg %></p>
  <div class="container">
    <!-- <div class="row">
      <div class="column">

      </div>
      <div class="column">
        
      </div>
    </div> -->
    <form action="/upload" method="post">
      <div class="form-group">
        <label for="firstName">First Name</label>
        <input
          type="text"
          name="survey[firstName]"
          id="firstName"
          class="form-control"
        />
      </div>

      <div class="form-group">
        <label for="surname">Surname</label>
        <input
          type="text"
          name="survey[surname]"
          id="name"
          class="form-control"
        />
      </div>

      <div class="form-group">
        <label for="phonenumber">Phone Number</label>
        <input
          type="text"
          name="survey[phonenumber]"
          id="phonenumber"
          class="form-control"
        />
      </div>

      <!-- <div class="form-group">
        <label for="birthday">BirthDay</label>
        <input
          type="text"
          name="survey[birthday]"
          id="birthday"
          class="form-control"
        />
      </div> -->
      <div class="form-group">
        <label for="birthday">Date of Birth:</label>
        <input
          type="date"
          id="birthday"
          name="survey[birthday]"
          class="form-control"
        />
      </div>

      <div class="form-group">
        <label for="gender">Gender</label>
        <select name="survey[gender]" id="gender">
          <option value="Male" selected>Male</option>
          <option value="Female">Female</option>
          class="form-control"
        </select>
        <!-- 
                  <input
          type="text"
          name="survey[gender]"
          id="gender"
          class="form-control"
        />
         -->
      </div>

      <div class="form-group">
        <label for="employmentstatus">Employment Status</label>
        <select name="survey[employmentstatus]" id="employmentstatus">
          <option value="Employed" selected>Employed</option>
          <option value="Not Employed">Not Employed</option>
          class="form-control"
        </select>
        <!-- <input
          type="text"
          name="survey[employmentstatus]"
          id="employmentstatus"
          class="form-control"
        /> -->
      </div>

      <div class="form-group">
        <label for="networth">Net Worth</label>
        <input
          type="networth"
          name="survey[networth]"
          id="networth"
          class="form-control"
        />
      </div>

      <div class="form-group">
        <label for="iownhome">Do you own a home?</label>
        <select name="survey[iownhome]" id="iownhome">
          <option value="Yes" selected>Yes</option>
          <option value="No">No</option>
          class="form-control"
        </select>
        <!-- <input
          type="text"
          name="survey[iownhome]"
          id="iownhome"
          class="form-control"
        /> -->
      </div>
      <button type="submit" class="btn.btn-primary" id="button">SUBMIT</button>
    </form>
  </div>
  <script
    src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN"
    crossorigin="anonymous"
  ></script>
</body>

<body>
  

<h3> <%= msg %> </h3>
<div class="container col-md-6">
<form>
<div class="form-group">
<label for="name">Name</label>
<input type="text" name="name" id="name" class="form-control">
</div>
 <div class="form-group">
  <label for="phonenumber">Phone Number</label>
  <input type="text" name="phonenumber" id="phonenumber" class="form-control">
 </div>
 <div class="form-group">
  <label for="birthday">BirthDay</label>
  <input type="text" name="birthday" id="birthday" class="form-control">
 </div>
 <div class="form-group">
  <label for="gender">Gender</label>
<input type="text" name="gender" id="gender" class="form-control">
</div>
<div class="form-group">
  <label for="employmentstatus">Employment Status</label>
<input type="text" name="employmentstatus" id="employmentstatus" class="form-control">
</div>
<div class="form-group">
  <label for="networth">Net Worth</label>
<input type="networth" name="networth" id="networth" class="form-control">
</div>
<div class="form-group">
  <label for="iownhome">Do you own a home?</label>
<input type="text" name="iownhome" id="iownhome" class="form-control">
</div>
</form>
</div>
